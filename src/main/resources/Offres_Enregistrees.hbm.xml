<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <!-- Mapping for OffresEnregistrees -->
    <class name="MesBeans.OffresEnregistrees" table="Offres_Enregistrees">
        <id name="idEnregistrement" column="id_enregistrement" type="integer">
            <generator class="identity"/>
        </id>
        <property name="idUtilisateur" column="id_utilisateur" type="integer"/>
        <property name="idOffre" column="id_offre" type="integer"/>
        <property name="dateEnregistrement" column="date_enregistrement" type="timestamp"/>
    </class>
</hibernate-mapping>

